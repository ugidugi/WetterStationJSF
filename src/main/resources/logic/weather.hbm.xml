<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="pojo">
    <class name="Weather" table="weather">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="date" column="date"/>
        <property name="directionWind" column="directionWind"/>
        <property name="speedWind" column="speedWind"/>
        <property name="temperature" column="temperature"/>
        <property name="humidity" column="humidity"/>
        <property name="radiation" column="radiation"/>
        <property name="pressure" column="pressure"/>
        <property name="precipitation" column="precipitation"/>
    </class>
</hibernate-mapping>